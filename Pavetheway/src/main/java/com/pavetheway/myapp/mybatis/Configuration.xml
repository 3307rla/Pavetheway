<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 자주 사용하는 type 의 별칭을 등록 해 놓고  Mapper xml 에서 사용할수 있다. -->
	<typeAliases>
		<typeAlias type="com.pavetheway.myapp.users.dto.UsersDto" alias="usersDto"/>
		<typeAlias type="com.pavetheway.myapp.shop.dto.ShopDto" alias="shopDto"/>
		<typeAlias type="com.pavetheway.myapp.review.dto.ReviewDto" alias="reviewDto"/>
		<typeAlias type="com.pavetheway.myapp.inquiry.dto.InquiryCommentDto" alias="inquiryCommentDto"/>
		<typeAlias type="com.pavetheway.myapp.inquiry.dto.InquiryDto" alias="inquiryDto"/>
		<typeAlias type="com.pavetheway.myapp.cart.dto.CartDto" alias="cartDto"/>
	</typeAliases>
	<!-- 필요한 Mapper xml 등록하기  -->
	<mappers>
		<mapper resource="com/pavetheway/myapp/mybatis/UsersMapper.xml"/>
		<mapper resource="com/pavetheway/myapp/mybatis/ShopMapper.xml"/>
		<mapper resource="com/pavetheway/myapp/mybatis/ReviewMapper.xml"/>
		<mapper resource="com/pavetheway/myapp/mybatis/InquiryMapper.xml"/>
		<mapper resource="com/pavetheway/myapp/mybatis/InquiryCommentMapper.xml"/>
		<mapper resource="com/pavetheway/myapp/mybatis/CartMapper.xml"/>
	</mappers>
</configuration>